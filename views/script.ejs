<% setDate = function() {
    var month = new Array();
    month[0] = "January";
    month[1] = "February";
    month[2] = "March";
    month[3] = "April";
    month[4] = "May";
    month[5] = "June";
    month[6] = "July";
    month[7] = "August";
    month[8] = "September";
    month[9] = "October";
    month[10] = "November";
    month[11] = "December";

    var days = ["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"];

    var d = new Date();
    var mm = month[d.getMonth()];
    var dd = d.getDate();
    var day = days[d.getDay()];
    var dateString = day+", "+mm+" "+dd;
    return dateString;
  }
%>